{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "PatchMaker": false,
    "Handlebars": false,
    "Popup" : false,
    "Bubble" : false,
    "jQuery" : false,
    "App" : false,
    "$" : false
  },
  "extends": "eslint:recommended"
}